#include <Ultrasonic.h>

const int soundPin = 6;
int triggerPin =  AN1;
int echoPin =     AN0;
Ultrasonic ultrasonic(triggerPin, echoPin, BUSA);

int readingValue;
int frequency;



void setup()
  {
  Serial.begin(9600);
  }

void loop()
  {
  float cmMsec, inMsec;
  long microsec = ultrasonic.timing(500, Ultrasonic::IN);

  cmMsec = ultrasonic.convert(microsec, Ultrasonic::CM);
  inMsec = ultrasonic.convert(microsec, Ultrasonic::IN);
  Serial.print("MS: ");
  Serial.print(microsec);
  Serial.print(", CM: ");
  Serial.print(cmMsec);
  Serial.print(", IN: ");
  Serial.println(inMsec);
  delay(100);

  readingValue = analogRead(microsec);

  tone(soundPin, readingValue);
  

  
  }
